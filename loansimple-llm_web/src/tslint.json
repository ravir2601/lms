{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "kt",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "kt",
            "kebab-case",
            "camelCase"
        ],
        "variable-name": [
            true,
            "allow-snake-case"
        ],
        "no-string-literal": false
    }
}
